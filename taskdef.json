{
  "family": "task-app-python",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::777422376006:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "python-app",
      "image": "777422376006.dkr.ecr.us-east-1.amazonaws.com/python_app:latest",
      "cpu": 1024,
      "memory": 3072,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/task-app-python",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072"
}
